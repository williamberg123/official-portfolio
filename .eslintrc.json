{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
			"no-tabs": "off",
			"react/react-in-jsx-scope": "off",
			"import/prefer-default-export": "off",
			"react/jsx-filename-extension": "off",
			"react/jsx-indent": "off",
			"indent": "off",
			"comma-dangle": "off",
			"react/jsx-one-expression-per-line": "off",
			"react/jsx-indent-props": "off"
		}
}
